<h1>Houses</h1>

<% @houses.each do |house| %>

  <ul><%=link_to "#{house.name}", house %>

    <li><%=house.castle %></li>

    <% if current_user %>
      <% if house.user_id == current_user.id %>
        <li>Created by you!</li>
        <% end %>
    <%else%>
    <% end %>

    </ul>
<% end %>

<%= link_to "Establish New House", new_house_path %>
